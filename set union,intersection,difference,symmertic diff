n = int(input())
roll_n = set(map(int,input().split()))
b = int(input())
roll_b = map(int,input().split())
print(len(roll_n.union(roll_b)))

/////intersection

n = int(input())
roll_n = set(map(int,input().split()))
b = int(input())
roll_b = map(int,input().split())
print(len(roll_n.union(roll_b)))

