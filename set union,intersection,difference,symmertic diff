# Enter your code here. Read input from STDIN. Print output to STDOUT
n = int(input())
roll_n = set(map(int,input().split()))
b = int(input())
roll_b = set(map(int,input().split()))
print(len(roll_n-roll_b))

//symmetric difference

n = int(input())
roll_n = set(map(int,input().split()))
b = int(input())
roll_b = set(map(int,input().split()))
print(len(roll_n^roll_b))
